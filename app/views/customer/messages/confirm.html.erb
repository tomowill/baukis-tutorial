<% @title = '新規問い合わせ（確認）' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @message, url: :customer_messages do |f| %>
    <%= render 'confirming_form', f: f%>
    <div class="button">
      <%= f.submit '送信' %>
      <%= f.submit '訂正', name: 'correct' %>
      <%= link_to 'キャンセル', :customer_root %>
    </div>
  <% end %>
</div>
